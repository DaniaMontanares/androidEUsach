<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/loginn"
        android:id="@+id/textViewLogMain"
        android:textSize="@dimen/design_fab_image_size"
        android:textColor="@color/colorPrimaryDark"
        android:layout_marginRight="79dp"
        android:layout_marginEnd="79dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="text_msg2"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Registrarse"
        android:id="@+id/textView4"
        android:layout_below="@+id/textViewLogMain"
        android:layout_alignLeft="@+id/textViewLogMain"
        android:layout_alignStart="@+id/textViewLogMain"
        android:textSize="@dimen/design_fab_image_size"
        android:textColor="@color/colorPrimaryDark"
        android:onClick="text_msg3"/>
</RelativeLayout>